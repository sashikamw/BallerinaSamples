package Passtrough;

import ballerina.lang.messages;
import ballerina.net.http;

service nyseStockQuoteservice {
    
    @http:POST{}
    resource stocks (message m) {
        message response = {};
        json payload = {"exchange":"nyse", "name":"IBM", "value":"127.50"};
        messages:setJsonPayload(response, payload);
        reply response;
        
    }
    
}
